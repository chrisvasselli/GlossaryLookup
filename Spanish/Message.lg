    <TextItem>
      <Description> Description to provide password for an account. The argument is the account’s name. </Description>
      <Position>PROVIDE_PASSWORD_FOR_ACCOUNT_FORMAT</Position>
      <TranslationSet>
        <base loc="en">Enter the password for the account “%@”.</base>
        <tran loc="es">Introduce la contraseña de la cuenta “%@”.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Sender name format (used in attribution reply prefix) for when the sender has a full name and email address. The first argument is the sender’s full name, the second is their email address. </Description>
      <Position>REPLY_ATTRIBUTION_SENDER</Position>
      <TranslationSet>
        <base loc="en">%1$@ &lt;%2$@&gt;</base>
        <tran loc="es">%1$@ &lt;%2$@&gt;</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> String added to message for compose on iPod </Description>
      <Position>DEFAULT_SIGNATURE_IPOD</Position>
      <TranslationSet>
        <base loc="en">Sent from my iPod</base>
