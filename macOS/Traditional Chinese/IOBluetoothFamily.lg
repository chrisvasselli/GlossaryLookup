<?xml version="1.0" encoding="UTF-8"?>
<Proj>
  <ProjName>IOBluetoothFamily</ProjName>
  <File>
    <Filepath>IOBluetoothFamily/System/Library/Frameworks/IOBluetooth.framework/Versions/A/Resources/en.lproj/Localizable.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>keyTurnBluetoothOff</Position>
      <TranslationSet>
        <base loc="en">Turn Bluetooth Off</base>
        <tran loc="zh-TW">關閉藍牙</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyShouldNotTurnOffBluetoothReason</Position>
      <TranslationSet>
        <base loc="en">For example, you won’t be able to use a Bluetooth keyboard or mouse. Are you sure you want to turn Bluetooth off?</base>
        <tran loc="zh-TW">例如，您將無法使用藍牙鍵盤或滑鼠。您確定要關閉藍牙嗎？</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyOK</Position>
      <TranslationSet>
        <base loc="en">OK</base>
        <tran loc="zh-TW">好</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>kUsersMouseKey</Position>
      <TranslationSet>
        <base loc="en">%@’s Mouse</base>
        <tran loc="zh-TW">%@的滑鼠</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyCancel</Position>
      <TranslationSet>
        <base loc="en">Cancel</base>
        <tran loc="zh-TW">取消</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyLeaveBluetoothOn</Position>
      <TranslationSet>
        <base loc="en">Leave Bluetooth On</base>
        <tran loc="zh-TW">保持藍牙開啟</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyShouldNotTurnOffBluetooth</Position>
      <TranslationSet>
        <base loc="en">If you turn Bluetooth off you will not be able to use your Bluetooth devices.</base>
        <tran loc="zh-TW">若您關閉藍牙，您將無法使用藍牙裝置。</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyCannotTurnOffBluetoothReason</Position>
      <TranslationSet>
        <base loc="en">You cannot control your computer if you turn Bluetooth off at this time as you would lose your input devices.</base>
        <tran loc="zh-TW">若此時關閉藍牙，您將無法控制電腦，且輸入裝置與電腦的連線也會中斷。</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>Unknown</Position>
      <TranslationSet>
        <base loc="en">Unknown</base>
        <tran loc="zh-TW">未知的</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>keyCannotTurnOffBluetooth</Position>
      <TranslationSet>
        <base loc="en">You cannot control your computer if you turn Bluetooth off at this time. </base>
        <tran loc="zh-TW">若此時關閉藍牙，您將無法控制電腦。 </tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>kUsersTrackpadKey</Position>
      <TranslationSet>
        <base loc="en">%@’s Trackpad</base>
        <tran loc="zh-TW">%@的觸控式軌跡板</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>kUsersKeyboardKey</Position>
      <TranslationSet>
        <base loc="en">%@’s Keyboard</base>
        <tran loc="zh-TW">%@的鍵盤</tran>
      </TranslationSet>
    </TextItem>
  </File>
</Proj>
