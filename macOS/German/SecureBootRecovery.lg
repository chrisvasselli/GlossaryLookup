    <TextItem>
      <Description/>
      <Position>PERSONALIZE_VOLUME_UNLOCK_FAILED_TITLE_FORMAT</Position>
      <TranslationSet>
        <base loc="en">“%@” is encrypted and must be unlocked.</base>
        <tran loc="de">„%@“ ist verschlüsselt und muss entsperrt werden.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>PERSONALIZE_VOLUME_UNLOCK_FAILED_DESCRIPTION</Position>
      <TranslationSet>
        <base loc="en">You must unlock this disk to use it to start up.</base>
        <tran loc="de">Du musst diese Festplatte entsperren, um sie als Startfestplatte zu verwenden.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>OK_BUTTON</Position>
      <TranslationSet>
        <base loc="en">OK</base>
        <tran loc="de">OK</tran>
      </TranslationSet>
    </TextItem>
