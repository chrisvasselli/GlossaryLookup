    <TextItem>
      <Description> Localized versions of Info.plist keys </Description>
      <Position>CFBundleName</Position>
      <TranslationSet>
        <base loc="en">CoreLocation</base>
        <tran loc="fr">CoreLocation</tran>
      </TranslationSet>
    </TextItem>
  </File>
  <File>
    <Filepath>CoreLocationFramework/System/Library/Frameworks/CoreLocation.framework/Versions/A/Resources/en.lproj/locationd.stringsdict</Filepath>
    <TextItem>
      <Description/>
      <Position>/LOCATION_CLIENT_PERMISSION_REPROMPT_OTHER:dict/LOCATION_COUNT:dict/one:dict/:string</Position>
      <TranslationSet>
        <base loc="en">%lu times</base>
        <tran loc="fr">%lu fois</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>/LOCATION_CLIENT_PERMISSION_REPROMPT_OTHER:dict/LOCATION_COUNT:dict/other:dict/:string</Position>
      <TranslationSet>
        <base loc="en">%lu times</base>
        <tran loc="fr">%lu fois</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>/LOCATION_CLIENT_PERMISSION_REPROMPT_OTHER:dict/NSStringLocalizedFormatKey:dict/:string</Position>
      <TranslationSet>
        <base loc="en">“%@” has used your location %#@LOCATION_COUNT@ in the background over the past 3 days. Do you want to continue to allow background location use?</base>
        <tran loc="fr">« %@ » a utilisé votre position %#@LOCATION_COUNT@ en arrière-plan au cours des 3 derniers jours. L’autorisez-vous à continuer à utiliser votre position en arrière-plan ?</tran>
      </TranslationSet>
    </TextItem>
  </File>
</Proj>
