<?xml version="1.0" encoding="UTF-8"?>
<Proj>
  <ProjName>MCXSecurityPlugin</ProjName>
  <File>
    <Filepath>MCXSecurityPlugin/System/Library/CoreServices/SecurityAgentPlugins/MCXMechanism.bundle/Contents/Resources/English.lproj/localizable.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_MOUNT_MOBILE_ACCNT_DEVICE_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem accessing your mobile account volume.</base>
        <tran loc="fr">Un problème est survenu lors de l’accès au volume de votre compte mobile.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_LOCAL_NOT_ALLOWED_TEXT</Position>
      <TranslationSet>
        <base loc="en">Local users are not allowed to log in.   Please contact your system administrator.</base>
        <tran loc="fr">Les utilisateurs locaux ne sont pas autorisés à ouvrir une session. Veuillez contacter votre administrateur système.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_FILEVAULT_HOME_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating a FileVault home.</base>
        <tran loc="fr">Un problème est survenu lors de la création d’un dossier de départ FileVault.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating your mobile account.</base>
        <tran loc="fr">Un problème est survenu lors de la création de votre compte mobile.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_MEMBERSHIP_DENIED_TEXT</Position>
      <TranslationSet>
        <base loc="en">Your current settings do not allow you to log in.   Please contact your system administrator.</base>
        <tran loc="fr">Les réglages en vigueur de votre compte ne vous autorisent pas à ouvrir une session. Veuillez contacter votre administrateur système.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_SIMULTANEOUS_TEXT</Position>
      <TranslationSet>
        <base loc="en">You may only log in on one computer at a time.   Please contact your system administrator or try again later.</base>
        <tran loc="fr">Vous ne pouvez ouvrir une session que sur un ordinateur à la fois. Contactez votre administrateur système ou réessayez plus tard.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Alert strings </Description>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Unable to create mobile account.</base>
        <tran loc="fr">Création de compte mobile impossible.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_LOCAL_NOT_ALLOWED_TITLE</Position>
      <TranslationSet>
        <base loc="en">You are not allowed to log in at this time.</base>
        <tran loc="fr">Vous n’êtes actuellement pas autorisé à ouvrir une session.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_MEMBERSHIP_DENIED_TITLE</Position>
      <TranslationSet>
        <base loc="en">You are not allowed to log in at this time.</base>
        <tran loc="fr">Vous n’êtes actuellement pas autorisé à ouvrir une session.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_RECORD_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating your mobile account record.</base>
        <tran loc="fr">Un problème est survenu lors de la création de la fiche de votre compte mobile.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_BAD_MOBILE_ACCNT_PATH_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem while creating or accessing “%@”.</base>
        <tran loc="fr">Un problème est survenu lors de la création de « %@ » ou lors de l’accès à cet élément.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_BAD_FILEVAULT_HOME_PATH_TEXT</Position>
      <TranslationSet>
        <base loc="en">Your FileVault home cannot be created because a folder with the same name already exists.</base>
        <tran loc="fr">Votre dossier de départ FileVault ne peut pas être créé car il existe déjà un dossier portant le même nom.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_SIMULTANEOUS_TITLE</Position>
      <TranslationSet>
        <base loc="en">You cannot continue because you are already logged in on another computer.</base>
        <tran loc="fr">Impossible de poursuivre car vous avez ouvert une session sur un autre ordinateur.</tran>
      </TranslationSet>
    </TextItem>
  </File>
</Proj>
