<?xml version="1.0" encoding="UTF-8"?>
<Proj>
  <ProjName>MCXSecurityPlugin</ProjName>
  <File>
    <Filepath>MCXSecurityPlugin/System/Library/CoreServices/SecurityAgentPlugins/MCXMechanism.bundle/Contents/Resources/English.lproj/localizable.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_MOUNT_MOBILE_ACCNT_DEVICE_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem accessing your mobile account volume.</base>
        <tran loc="es">Se ha producido un problema al acceder a tu volumen de cuenta móvil.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_LOCAL_NOT_ALLOWED_TEXT</Position>
      <TranslationSet>
        <base loc="en">Local users are not allowed to log in.   Please contact your system administrator.</base>
        <tran loc="es">Los usuarios locales no están autorizados para iniciar una sesión. Ponte en contacto con el administrador del sistema.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_FILEVAULT_HOME_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating a FileVault home.</base>
        <tran loc="es">Se ha producido un problema al crear una carpeta de inicio FileVault.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating your mobile account.</base>
        <tran loc="es">Se ha producido un problema al crear tu cuenta móvil.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_MEMBERSHIP_DENIED_TEXT</Position>
      <TranslationSet>
        <base loc="en">Your current settings do not allow you to log in.   Please contact your system administrator.</base>
        <tran loc="es">Tus ajustes actuales no te permiten iniciar una sesión. Ponte en contacto con el administrador del sistema.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_SIMULTANEOUS_TEXT</Position>
      <TranslationSet>
        <base loc="en">You may only log in on one computer at a time.   Please contact your system administrator or try again later.</base>
        <tran loc="es">No puedes iniciar una sesión en más de un ordenador a la vez. Ponte en contacto con el administrador del sistema o inténtalo más tarde.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Alert strings </Description>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Unable to create mobile account.</base>
        <tran loc="es">No se ha podido crear la cuenta móvil.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_LOCAL_NOT_ALLOWED_TITLE</Position>
      <TranslationSet>
        <base loc="en">You are not allowed to log in at this time.</base>
        <tran loc="es">No está autorizado para iniciar una sesión en este momento.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_MEMBERSHIP_DENIED_TITLE</Position>
      <TranslationSet>
        <base loc="en">You are not allowed to log in at this time.</base>
        <tran loc="es">No está autorizado para iniciar una sesión en este momento.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_CANT_CREATE_MOBILE_ACCNT_RECORD_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem creating your mobile account record.</base>
        <tran loc="es">Se ha producido un problema al crear tu registro de cuenta móvil.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_BAD_MOBILE_ACCNT_PATH_TEXT</Position>
      <TranslationSet>
        <base loc="en">There was a problem while creating or accessing “%@”.</base>
        <tran loc="es">Se ha producido un problema al crear o intentar acceder a “%@”.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_BAD_FILEVAULT_HOME_PATH_TEXT</Position>
      <TranslationSet>
        <base loc="en">Your FileVault home cannot be created because a folder with the same name already exists.</base>
        <tran loc="es">Tu carpeta de inicio FileVault no puede crearse porque ya existe una carpeta con el mismo nombre.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MCX_USER_CANT_LOGIN_SIMULTANEOUS_TITLE</Position>
      <TranslationSet>
        <base loc="en">You cannot continue because you are already logged in on another computer.</base>
        <tran loc="es">No puede continuar porque ya ha iniciado una sesión en otro ordenador.</tran>
      </TranslationSet>
    </TextItem>
  </File>
</Proj>
