    <TextItem>
      <Description/>
      <Position>BUTTON_TITLE</Position>
      <TranslationSet>
        <base loc="en">About Location Services &amp; Privacy…</base>
        <tran loc="es">Acerca de la localización y la privacidad…</tran>
