    <Filepath>EFIResourceBuilder/System/Library/PrivateFrameworks/EFILogin.framework/Versions/A/Resources/EFIResourceBuilder.bundle/Contents/Resources/English.lproj/Localizable.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>IF_YOU_FORGOT_HINT_TITLE</Position>
      <TranslationSet>
        <base loc="en">If you forgot your password, you can…</base>
        <tran loc="es">Si has olvidado la contraseña, puedes…</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RATE_LIMIT_LOCKED_OUT</Position>
      <TranslationSet>
        <base loc="en">Your account is locked.</base>
        <tran loc="es">Tu cuenta está bloqueada.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>FIRST_BOOT_WARNING_MSG</Position>
      <TranslationSet>
        <base loc="en">To begin encrypting your disk, log in to your account. If you can’t log in, press the power button on your Mac to shut it down. Disk encryption will be canceled.</base>
        <tran loc="es">Para iniciar la encriptación del disco, inicia sesión en tu cuenta. Si no puedes iniciar sesión, pulsa el botón de encendido del Mac para apagarlo. La encriptación del disco se cancelará.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>NAME_PLACEHOLDER</Position>
      <TranslationSet>
        <base loc="en">Name</base>
        <tran loc="es">Nombre</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESET</Position>
      <TranslationSet>
        <base loc="en">Reset…</base>
        <tran loc="es">Restablecer…</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESTART_TO_SAFARI_WARNING_BUTTON</Position>
      <TranslationSet>
        <base loc="en">Restart without saving changes</base>
        <tran loc="es">Reiniciar sin guardar los cambios</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DISK_PASSWORD</Position>
      <TranslationSet>
        <base loc="en">Disk Password</base>
        <tran loc="es">Contraseña del disco</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>SHUT_DOWN</Position>
      <TranslationSet>
        <base loc="en">Shut Down</base>
        <tran loc="es">Apagar</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESET_USING_RECOVERY_LINK</Position>
      <TranslationSet>
        <base loc="en">…reset it using your Recovery Key</base>
        <tran loc="es">… cambiarla mediante la clave de recuperación</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RECOVERY_USER</Position>
      <TranslationSet>
        <base loc="en">Enter Recovery Key</base>
        <tran loc="es">Introducir clave de recuperación</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CALL_SOMEONE_ELSE</Position>
      <TranslationSet>
        <base loc="en">Your recovery key has been archived at &lt;%@&gt;. Contact your system administrator to retrieve it. You may be asked to provide the following information:</base>
        <tran loc="es">Tu clave de recuperación se ha archivado en &lt;%@&gt;. Ponte en contacto con el administrador del sistema para recuperarla. Puede que se te solicite la información siguiente:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>PASSWORD_HINT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Password Hint</base>
        <tran loc="es">Indicación de la contraseña</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Labels for Recovery Key information</Description>
      <Position>RECOVERY_KEY_SERIAL_NUMBER</Position>
      <TranslationSet>
        <base loc="en">Serial Number:</base>
        <tran loc="es">Número de serie:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MASTERPASSWORD_PLACEHOLDER</Position>
      <TranslationSet>
        <base loc="en">Master Password</base>
        <tran loc="es">Contraseña maestra</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MASTER_PASSWORD</Position>
      <TranslationSet>
        <base loc="en">Master Password</base>
        <tran loc="es">Contraseña maestra</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Fed in stringFromTimeInterval from NSDateComponentsFormatter (NSDateComponentsFormatterUnitsStyleFull)</Description>
      <Position>RATE_LIMIT_BACKOFF_FORMAT</Position>
      <TranslationSet>
        <base loc="en">Your account is locked. Try again in %@.</base>
        <tran loc="es">Tu cuenta está bloqueada. Inténtalo de nuevo dentro de %@.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESTART</Position>
      <TranslationSet>
        <base loc="en">Restart</base>
        <tran loc="es">Reiniciar</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESTART_TO_SAFARI_INFO_BUTTON</Position>
      <TranslationSet>
        <base loc="en">Restart</base>
        <tran loc="es">Reiniciar</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESTART_TO_SAFARI_WARNING_TITLE</Position>
      <TranslationSet>
        <base loc="en">This computer will restart to a secure, Safari-only system for the Guest user. Other users logged into this computer may lose unsaved changes.</base>
        <tran loc="es">El ordenador se reiniciará con un sistema seguro para el usuario invitado, con Safari únicamente. Puede que los demás usuarios con una sesión iniciada en este ordenador pierdan los cambios no guardados.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>PASSWORD_IS_REQUIRED_TOUCHID_MESSAGE</Position>
      <TranslationSet>
        <base loc="en">Touch ID requires your password when Mac restarts</base>
        <tran loc="es">Cuando el Mac se reinicia, Touch ID requiere la contraseña</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>PASSWORD_PLACEHOLDER</Position>
      <TranslationSet>
        <base loc="en">Enter Password</base>
        <tran loc="es">Contraseña</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>OTHER_INPUT_SOURCES</Position>
      <TranslationSet>
        <base loc="en">Other Input Sources</base>
        <tran loc="es">Otras fuentes de entrada</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CALL_APPLE_CARE</Position>
      <TranslationSet>
        <base loc="en">To obtain your recovery key you need to call AppleCare. Visit https://www.apple.com/support/ for support options and phone numbers in your region. When calling, you will be asked to provide the following information:</base>
        <tran loc="es">Para obtener la clave de recuperación debes llamar a AppleCare. Visita https://support.apple.com/es-es para consultar las opciones de soporte y los números de teléfono de tu región. Cuando llames, se te pedirá que facilites la siguiente información:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>UNKNOWN_USER</Position>
      <TranslationSet>
        <base loc="en">[Update Needed]</base>
        <tran loc="es">[Actualización necesaria]</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESTART_TO_SAFARI_INFO_TITLE</Position>
      <TranslationSet>
        <base loc="en">This computer will restart to a secure, Safari-only system for the Guest user.</base>
        <tran loc="es">El ordenador se reiniciará con un sistema seguro para el usuario invitado, con Safari únicamente.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RECOVERYKEY_PLACEHOLDER</Position>
      <TranslationSet>
        <base loc="en">Recovery Key</base>
        <tran loc="es">Clave de recuperación</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RECOVERY_KEY_RECORD_NUMBER</Position>
      <TranslationSet>
        <base loc="en">Record Number:</base>
        <tran loc="es">Número de registro:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RECOVERY_HINT_WITH_HINT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Type the recovery key.  Recovery key hint:</base>
        <tran loc="es">Introduce la clave de recuperación. Indicación de la clave de recuperación:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>UNKNOWN_USER_HINT</Position>
      <TranslationSet>
        <base loc="en">The user Graphics resources appears to be invalid</base>
        <tran loc="es">Parece que los recursos gráficos del usuario no son válidos</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESET_USING_MASTER_PASSWORD_LINK</Position>
      <TranslationSet>
        <base loc="en">…reset it using your Master Password</base>
        <tran loc="es">… cambiarla mediante la contraseña maestra</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RESET_USING_ICLOUD_LINK</Position>
      <TranslationSet>
        <base loc="en">…reset it using your Apple ID</base>
        <tran loc="es">… cambiarla mediante tu ID de Apple</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MASTER_PASSWORD_HINT_WITH_HINT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Type the master password to reset this user’s password.  Master password hint:</base>
        <tran loc="es">Introduce la contraseña maestra para cambiar la contraseña de este usuario. Indicación de la contraseña maestra:</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>RECOVERY_HINT_TITLE</Position>
      <TranslationSet>
        <base loc="en">Type the recovery key</base>
        <tran loc="es">Introduce la clave de recuperación</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>MASTER_PASSWORD_HINT_TITLE</Position>
