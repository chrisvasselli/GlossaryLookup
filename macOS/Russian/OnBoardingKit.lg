    <TextItem>
      <Description/>
      <Position>SPLASH_TITLE</Position>
      <TranslationSet>
        <base loc="en">Apple Podcasts &amp; Privacy</base>
        <tran loc="ru">Apple Podcasts и конфи­денциальность</tran>
