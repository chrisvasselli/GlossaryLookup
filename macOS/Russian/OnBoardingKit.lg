    <TextItem>
      <Description/>
      <Position>SPLASH_TITLE</Position>
      <TranslationSet>
        <base loc="en">Data &amp; Privacy</base>
        <tran loc="ru">Данные и конфи­денциальность</tran>
      </TranslationSet>
    </TextItem>
