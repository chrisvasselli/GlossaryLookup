      <TranslationSet>
        <base loc="en">Trash</base>
        <tran loc="fr">Corbeille</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Description to provide password for an account. The argument is the account’s name. </Description>
      <Position>PROVIDE_PASSWORD_FOR_ACCOUNT_FORMAT</Position>
      <TranslationSet>
        <base loc="en">Enter the password for the account “%@”.</base>
        <tran loc="fr">Saisissez le mot de passe du compte « %@ ».</tran>
      </TranslationSet>
    </TextItem>
