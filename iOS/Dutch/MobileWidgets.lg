        <base loc="en">%#@widgets@</base>
        <tran loc="nl">%#@widgets@</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>/NEW_WIDGETS_AVAILABLE:dict/widgets:dict/one:dict/:string</Position>
      <TranslationSet>
        <base loc="en">%u New Widget Available</base>
        <tran loc="nl">%u nieuwe widget beschikbaar</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>/NEW_WIDGETS_AVAILABLE:dict/widgets:dict/other:dict/:string</Position>
