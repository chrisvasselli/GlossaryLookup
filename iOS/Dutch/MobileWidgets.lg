    <TextItem>
      <Description/>
      <Position>WIDGETS_EDIT_HEADLINE</Position>
      <TranslationSet>
        <base loc="en">Add Widgets</base>
        <tran loc="nl">Voeg widgets toe</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>WIDGETS_EDIT_TODAY_HEADLINE</Position>
      <TranslationSet>
        <base loc="en">Today View</base>
        <tran loc="nl">Vandaag-weergave</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>WIDGETS_EDIT_DISABLED</Position>
      <TranslationSet>
        <base loc="en">More Widgets</base>
        <tran loc="nl">Meer widgets</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>WIDGETS_EDIT_FAVORITES_DESCRIPTION</Position>
      <TranslationSet>
        <base loc="en">Pinned favorites will always appear on the Home Screen. Swipe up to see the rest.</base>
        <tran loc="nl">Vastgemaakte favorieten staan altijd boven aan het beginscherm. Veeg omhoog om de rest te zien.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>WIDGETS_PIN_TEACHING_YES_BUTTON</Position>
      <TranslationSet>
        <base loc="en">Add to Home Screen</base>
        <tran loc="nl">Zet op beginscherm</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
