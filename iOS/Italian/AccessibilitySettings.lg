  <File>
    <Filepath>AccessibilitySettings/System/Library/PreferenceBundles/AccessibilitySettings.bundle/en.lproj/CommandAndControlSettings.strings</Filepath>
    <TextItem>
      <Description/>
      <Position>COMMAND_AND_CONTROL_FOOTER_TEXT</Position>
      <TranslationSet>
        <base loc="en">Voice Control allows you to use your voice to control your iOS device.</base>
        <tran loc="it">La funzionalità “Controllo vocale” ti consente di usare la voce per gestire il tuo dispositivo iOS.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_GROUP_FOOTER</Position>
      <TranslationSet>
        <base loc="en">Command hints help guide you as you learn Voice Control by suggesting commands and offering hints along the way.</base>
        <tran loc="it">I suggerimenti relativi ai comandi ti consentono di imparare a usare la funzionalità “Controllo vocale” facilmente, suggerendoti alcuni comandi e proponendoti consigli mentre la usi.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_GROUP_TITLE</Position>
      <TranslationSet>
        <base loc="en">Command Hints</base>
        <tran loc="it">Suggerimenti comandi</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>ATTENTION_AWARE_ACTION_SLEEP_WAKE_SUMMARY</Position>
      <TranslationSet>
        <base loc="en">Sleep/Wake Commands</base>
        <tran loc="it">Standby/Riattiva</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>VOCABULARY_FOOTER_TEXT</Position>
      <TranslationSet>
        <base loc="en">You can teach Voice Control new words by adding vocabulary entries.</base>
        <tran loc="it">Puoi aggiungere nuove parole a “Controllo vocale” creando nuove voci di dizionario.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>GRID_OVERLAY_COLUMNS_TITLE</Position>
      <TranslationSet>
        <base loc="en">Number of Columns</base>
        <tran loc="it">Numero di colonne</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_SHOW_SUGGESTED_COMMANDS_TITLE</Position>
      <TranslationSet>
        <base loc="en">Show Suggested Commands</base>
        <tran loc="it">Mostra i comandi suggeriti</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>GRID_OVERLAY_PRESS_IMMEDIATELY_TITLE</Position>
      <TranslationSet>
        <base loc="en">Tap by default</base>
        <tran loc="it">Tocca di default</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>FEATURE_AUTO</Position>
      <TranslationSet>
        <base loc="en">Automatic</base>
        <tran loc="it">Automatico</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>ATTENTION_AWARE_ACTION_LISTENING_OFF_ON_TITLE</Position>
      <TranslationSet>
        <base loc="en">Microphone Off; Microphone On</base>
        <tran loc="it">Microfono no; microfono sì</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DOWNLOADING_PROGRESS</Position>
      <TranslationSet>
        <base loc="en">To understand your voice commands, Voice Control needs information about your language. Downloading… %@ completed. %@ minutes remaining…</base>
        <tran loc="it">Per capire i tuoi comandi vocali, la funzionalità “Controllo vocale” ha bisogno di informazioni sulla lingua che usi. Download in corso… %@ completato. Mancano %@ minuti…</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DOWNLOAD_INFO_WLAN</Position>
      <TranslationSet>
        <base loc="en">To understand your voice commands, Voice Control will download information about your language when you are connected to WLAN.</base>
        <tran loc="it">Per capire i tuoi comandi vocali, la funzionalità “Controllo vocale” scarica le informazioni sulla lingua che usi quando ti connetti a una rete WLAN.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>OVERLAY_FADE_OPACITY_TITLE</Position>
      <TranslationSet>
        <base loc="en">Dimmed Opacity</base>
        <tran loc="it">Opacità attenuata</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_CLEAR_HISTORY_CONFIRM_TITLE</Position>
      <TranslationSet>
        <base loc="en">Clear all command history?</base>
        <tran loc="it">Confermi di voler cancellare tutta la cronologia comandi?</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DONE</Position>
      <TranslationSet>
        <base loc="en">Done</base>
        <tran loc="it">Fine</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_SHOW_NEXT_STEPS_TITLE</Position>
      <TranslationSet>
        <base loc="en">Show Helpful Tips</base>
        <tran loc="it">Mostra suggerimenti utili</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DOWNLOAD_INFO_WIFI</Position>
      <TranslationSet>
        <base loc="en">To understand your voice commands, Voice Control will download information about your language when you are connected to Wi-Fi.</base>
        <tran loc="it">Per capire i tuoi comandi vocali, la funzionalità “Controllo vocale” scarica le informazioni sulla lingua che usi quando ti connetti a una rete Wi-Fi.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>CREATE_NEW_CUSTOM_COMMAND</Position>
      <TranslationSet>
        <base loc="en">Create New Command…</base>
        <tran loc="it">Crea nuovo comando…</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>SETUP_COMMAND_AND_CONTROL</Position>
      <TranslationSet>
        <base loc="en">Set Up Voice Control</base>
        <tran loc="it">Imposta “Controllo vocale”</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_CLEAR_HISTORY_CONFIRM_DESC</Position>
      <TranslationSet>
        <base loc="en">Command history contains information about the commands you've learned, reducing the hints you see over time. Clear history if a new user wishes to learn Voice Control.</base>
        <tran loc="it">“Cronologia comandi” offre una panoramica dei comandi che hai imparato, in modo da ridurre il numero di suggerimenti che ti vengono proposti. Se un nuovo utente vuole imparare a usare la funzionalità “Controllo vocale”, è consigliabile cancellare la cronologia.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>GRID_OVERLAY_MAX_LEVEL_FOOTER</Position>
      <TranslationSet>
        <base loc="en">Saying the grid number at the maximum level of detail automatically performs a single tap.</base>
        <tran loc="it">Pronunciando il numero della griglia con il massimo livello di dettaglio, potrai eseguire automaticamente un tocco singolo.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>SHOW_TEXT_RESPONSE_TITLE</Position>
      <TranslationSet>
        <base loc="en">Show Confirmation</base>
        <tran loc="it">Mostra conferma</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>ANALYTICS_NOT_ALLOWED_FOOTER_TEXT</Position>
      <TranslationSet>
        <base loc="en">To help improve Voice Control, Siri or Dictation must be enabled.</base>
        <tran loc="it">Per aiutarci a migliorare “Controllo vocale”, è necessario abilitare Siri o Dettatura.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>DOWNLOADING</Position>
      <TranslationSet>
        <base loc="en">Downloading…</base>
        <tran loc="it">Scarico…</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_CLEAR_HISTORY_TITLE</Position>
      <TranslationSet>
        <base loc="en">Clear Command History</base>
        <tran loc="it">Cancella cronologia comandi</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>GRID_OVERLAY_GROUP_TITLE</Position>
      <TranslationSet>
        <base loc="en">Numbered Grid Overlay</base>
        <tran loc="it">Sovrapposizione griglia numerata</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>USER_HINTS_CLEAR_HISTORY_CONFIRM_BUTTON_TITLE</Position>
      <TranslationSet>
