        <tran loc="it">Privacy</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>ENVIRONMENTAL_AUDIO_ENABLED_LABEL</Position>
      <TranslationSet>
        <base loc="en">Environmental Sound Measurements</base>
        <tran loc="it">Misurazioni rumore ambientale</tran>
      </TranslationSet>
    </TextItem>
