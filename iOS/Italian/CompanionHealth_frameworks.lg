      <Position>TRENDS_NOTIFICATION_TITLE</Position>
      <TranslationSet>
        <base loc="en">Activity Trends</base>
        <tran loc="it">Trend attività</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>TRENDS_NOTIFICATION_BODY</Position>
      <TranslationSet>
        <base loc="en">You can view details in the Activity app on your iPhone.</base>
        <tran loc="it">Puoi visualizzare i dettagli nell'app Attività su iPhone.</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>TRENDS_NOTIFICATION_SUBTITLE</Position>
      <TranslationSet>
        <base loc="en">Your Trends are ready.</base>
        <tran loc="it">I tuoi Trend sono disponibili.</tran>
      </TranslationSet>
    </TextItem>
