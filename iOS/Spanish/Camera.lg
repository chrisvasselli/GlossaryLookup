      <TranslationSet>
        <base loc="en">Max (%@s)</base>
        <tran loc="es">Máx (%@ s)</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description> Aspect ratio controls </Description>
      <Position>ASPECT_SQUARE</Position>
      <TranslationSet>
        <base loc="en">Square</base>
        <tran loc="es">1:1</tran>
      </TranslationSet>
    </TextItem>
