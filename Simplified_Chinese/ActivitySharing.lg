    <TextItem>
      <Description/>
      <Position>/ACHIEVEMENT_ACHIEVED_FRIEND_DESCRIPTION:dict/NSStringLocalizedFormatKey:dict/:string</Position>
      <TranslationSet>
        <base loc="en">{{user_first_name}} just finished a 7-day competition. </base>
        <tran loc="zh-CN">{{user_first_name}}刚完成了一次7日竞赛。</tran>
      </TranslationSet>
    </TextItem>
    <TextItem>
      <Description/>
      <Position>/ACHIEVEMENT_TITLE:dict/NSStringLocalizedFormatKey:dict/:string</Position>
      <TranslationSet>
